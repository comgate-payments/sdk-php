{
  "name": "comgate/playground",
  "description": "Example project using ComGate Payments based on Nette",
  "license": "MIT",
  "type": "project",
  "repositories":[
    {
      "type": "path",
      "url": ".."
    }
  ],
  "require": {
    "php": ">=7.4",
    "contributte/application": "^0.5.0",
    "contributte/bootstrap": "^0.5.0",
    "contributte/di": "^0.5.0",
    "contributte/forms": "^0.5.0",
    "contributte/http": "^0.4.0",
    "contributte/mail": "^0.6.0",
    "contributte/utils": "^0.5.0",
    "contributte/latte": "^0.5.0",
    "contributte/tracy": "^0.5.0",
    "nette/robot-loader": "~3.4.0",
    "comgate/sdk": "^0.1.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.4.0",
    "contributte/qa": "^0.1",
    "contributte/dev": "^0.3",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-doctrine": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-phpunit": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0",
    "heroku/heroku-buildpack-php": "^192"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    },
    "files": ["app/bootstrap.php"]
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
