{
  "name": "comgate/sdk",
  "description": "ComGate Payments PHP SDK",
  "keywords": ["comgate", "payment", "php", "api", "money"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/comgate/sdk-php",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.3.4",
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.12.1",
    "phpstan/phpstan": "^0.12.2",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0",
    "psr/log": "^1.1.4"
  },
  "autoload": {
    "psr-4": {
      "Comgate\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
