{
    "name": "comgate/sdk",
    "description": "Comgate PHP SDK",
    "keywords": [
        "comgate",
        "payment",
        "php",
        "api",
        "money"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/comgate/sdk-php",
    "authors": [
        {
            "name": "Milan Felix Å ulc",
            "homepage": "https://f3l1x.io"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "psr/log": "^1.1|^2|^3",
        "psr/http-message": "^1.0.1|^2",
        "firebase/php-jwt": "^v6.1.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.2 || ^5.0",
        "codeception/module-asserts": "^1.3 || ^2.0 || ^3.0",
        "codeception/module-phpbrowser": "^1.0 || ^2.0",
        "codeception/stub": "^3.0 || ^4.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.5",
        "squizlabs/php_codesniffer": "^3.7",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Comgate\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Cases\\": "tests/cases",
            "Tests\\Toolkit\\": "tests/toolkit",
            "Tests\\PHPStanRule\\": "tests/PHPStanRule"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
